    <script>
        $(function () {
            //setup hubs
            var counterHub = $.connection.counterHub;
            $.connection.hub.start().done(function () {
                //do something after hub connected
            });
            //function receive data from server. server call in on code behind
            counterHub.client.UpdateCount = function (count) {
                $('#UserOnline').text(count);
            }
        });
        $(function () {
            var proxy = $.connection.notificationHub;
           
            proxy.client.receiveNotification = function (message) {
                if (message == "") $("#notify-template").empty().hide();
                else {
                    $("#notify-template").html("<mark class='notify' style='background: #17141dc9;'>" + message + "</mark>").show("slow");
                    var options = {
                        title: "Notificação",
                        options: {
                            body: message,
                            icon: "/images/shared/1/logo.png",
                            lang: 'pt-BR',
                        }
                    };
                    $("#easyNotify").easyNotify(options);
                }

            };
            $.connection.hub.start();
        });
    </script>
