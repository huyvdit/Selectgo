@using global::Web.Helpers
@using Web.Model.Models
<!DOCTYPE html>
<html lang="@DataCurrentHelper.UniqueSeoCode" class="sound-effects" data-click-ripple-animation="yes" data-audio-tick="/as-images/audio/tick.mp3">
<head>
    <title>..:: @DataCurrentHelper.Title(ViewBag.Title ?? ViewData["og:title"]) ::..</title>
    @{
        var gt = DataConfigsHelper.Get("GoogleWebmasterToolCode");
        if (gt != null)
        {
            @Html.Raw(gt)
        }
        var url = HttpContext.Current.Request.Url;
        GlobalVariables.OrderActive = false;
    }
    <link rel="dns-prefetch" href="//cdn.selectgo.vn">
    <link rel="icon" type="image/png" sizes="32x32" href="@DisplayHelper.Favicon">
    <link rel="manifest" href="~/assets/faviconsite.webmanifest">
    <link rel='shortlink' href='@DataConfigsHelper.LinksFrontEnd' />
    <link rel="canonical" href="@(DataConfigsHelper.LinksFrontEnd + "/" + DataCurrentHelper.UniqueSeoCode + url.AbsolutePath)" />

    @*<meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">*@
    <meta content="telephone=no" name="format-detection">
    <meta name="HandheldFriendly" content="true">
    @{
        Html.RenderPartial("~/Views/Default/Shared/Component/_MetaHead.cshtml");
    }
    @Styles.Render("~/bundles/css/core")

</head>
<body class="alt-bg">
    <main id="main-scrollbar" data-scrollbar>
        @*<a href="@Url.ActionEnc("mySecret",  "Encrypt", "System",new {id=7, a = 1, b = "asd" })" data-href-seo="@Url.ActionEnc(new ASP.Web.Core.App_Start.EncDecSEOBull(), "EncryptSEO",  "System", new {id=7, a = 1, b = "asd" })"></a>*@
        <!-- Page Loader -->
        <div class="preloader-wrapper preloader-it">

            <div class="preloader" style="text-align:center;line-height:1.71429;">
                @*<img src="@Url.ContentCdn("~/images/shared/all/loading-insight.gif")" width="128" />*@
                <!-- Preloader -->
                <div class="cws_loader">
                    <span>Waiting</span>
                    <div class="hex"></div>
                    <div class="hex"></div>
                    <div class="hex"></div>
                    <div class="hex"></div>
                    <div class="hex"></div>
                    <div class="hex"></div>
                    <div class="hex"></div>
                </div>
                <p class="sm-text-center" style="margin-top:-15px;font-family: sans-serif;">
                    @LanguageHelper.Translate("Loading to Insight_Text")...
                </p>
                <em style="font-family: sans-serif;top:0px!important; opacity:0; margin-top:0px!important; line-height: 1.71429!important;font-size: 9px!important;background: #dee2e6;padding: 8px 24px;border-radius: 0px 0px 5px 5px;color: #4caf50;font-style: normal;">Loading depends on your connection speed!</em>

            </div>
        </div>

        <!-- main start -->
        <section class="site all" style="display:none; opacity: 0;">

            <div class="preloader">
                <div class="inner" data-tilt data-tilt-perspective="2000">
                    <span class="typewriter" id="typewriter"></span>
                </div>
                <!-- end inner -->
            </div>
            <!-- end preloader -->
            <div class="transition-overlay">
                <div class="layer"></div>
                <div class="layer"></div>
                <div class="layer"></div>
                <div class="layer"></div>
            </div>
            <!-- end transition-overlay -->
            <div class="navigation-menu">
                <div class="bg-layers"> <span></span> <span></span> <span></span> <span></span> </div>
                <!-- end bg-layers -->
                <div class="inner" data-tilt data-tilt-perspective="2000">
                    <div class="menu">
                        <ul class="main-menu">
                            <li>
                                <a href="/" title="HOME">HOME <div class="subtitle">Home pages</div></a>
                                <ul>
                                    <li><a href="/#start">Start</a></li>
                                    <li><a href="/#recent-blog">Blog</a></li>
                                </ul>
                            </li>
                            <li><a href="/en/information.html" title="ABOUT US">ABOUT US <div class="subtitle">Infomation pages</div></a></li>
                            @if (Data.IsInRoleOrAuthenticated("services"))
                            {
                                <li><a href="/en/services" title="SERVICES">SERVICES <div class="subtitle">Services Collect</div></a></li>
                            }

                            @if (Data.IsInRoleOrAuthenticated("case-studies"))
                            {
                                <li><a href="/en/case-studies" title="CASE STUDIES">CASE STUDIES <div class="subtitle">Portfolios Collect</div></a></li>
                            }
                            <li><a href="/en/news" title="BLOG">BLOG <div class="subtitle">Recent Blog</div></a></li>
                            <li><a href="/en/contact-us.html" title="SAY HELLO">SAY HELLO <div class="subtitle">Contact Us</div></a></li>
                        </ul>
                    </div>
                    <!-- end menu -->
                    <blockquote>Let's create useful website for you ?</blockquote>
                </div>
                <!-- end inner -->
            </div>
            <!-- end navigation-menu -->

            <header class="header-desktop-fluid cd-main-header animate-search">
                <nav class="navbar">
                    @*<div class="left" style="text-align: center;opacity:0">
