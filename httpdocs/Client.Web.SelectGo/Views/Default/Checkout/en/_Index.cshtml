@{
    dynamic isAuthenticated = (Request.IsAuthenticated || ViewBag.Identifier != null);
    if (Model.Order.ShippingMethod == null)
    {
        Model.Order.ShippingMethod = new ShippingMethod() { Description = "{{DES}}", Fees = 0 };
    }
}
@using global::Web.Helpers
@using Web.Model.Models
@model Web.Core.Models.Ecommerce.View.CheckOutViewModel
@section styles{
    @Styles.Render("~/bundles/content/default/checkout/css")
}

<section class="js windows chrome desktop page--banner page--logo-banner page--show  svg opacity placeholder no-touchevents displaytable display-table generatedcontent cssanimations flexbox no-flexboxtweener anyflexbox no-shopemoji floating-labels">
    <a href="#main-header" class="skip-to-content">
        Skip to content
    </a>

    <header class="banner" data-header="" role="banner" aria-hidden="false">
        <div class="wrap">
            <a class="logo logo--left" href="https://geeks.selectgo.vn" title="「Sg.」"><svg width="200px" height="50px" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" viewBox="-5 15 250 25" version="1.1" style="    margin-top: -5px; border-left: 1px solid #fff;padding-left: 15px;"><title>Logo</title><desc>Created with love.</desc><g stroke="none" stroke-width="1" fill="none" fill-rule="evenodd"><g transform="translate(-315.000000, -38.000000)"><g transform="translate(315.000000, 38.000000)"><text id="Symbols" font-family="Quattrocento Sans,sans-serif" font-size="26" font-weight="500" fill="#fff"><tspan x="75" y="24">Select &amp; go</tspan><tspan x="189.14" y="24.5001221" font-weight="normal" fill="#e83f43" font-size="48">.</tspan><tspan x="75.16" y="46.5001221" font-family="" font-size="12" fill="#c90000" font-weight="normal"> checkout.</tspan></text><g id="Favicon"><circle fill="#fff" cx="20" cy="19.9917" r="4"></circle><path fill="#fff" d="M20,0.0073c-11.0469,0-20,8.9551-20,20c0,5.5762,2.2891,10.6094,5.9688,14.2344L20,48.2729l5.6602-5.6602L12.0938,28.9526C9.6035,26.7534,8,23.5737,8,19.9917c0-6.6289,5.375-12.002,12-12.002s12,5.373,12,12.002c0,4.7734-2.8125,8.8633-6.8516,10.7969l6.168,6.168l2.7148-2.7148C37.7109,30.6167,40,25.5835,40,20.0073C40,8.9624,31.0469,0.0073,20,0.0073z"></path></g></g></g></g></svg></a>
            <h1 class="visually-hidden">
                Information
            </h1>
        </div>
    </header>

    <aside role="complementary">
        <button class="order-summary-toggle order-summary-toggle--show shown-if-js" aria-expanded="false" aria-controls="order-summary" data-drawer-toggle="[data-order-summary]" aria-hidden="false">
            <span class="wrap">
                <span class="order-summary-toggle__inner">
                    <span class="order-summary-toggle__icon-wrapper">
                        <svg width="20" height="19" xmlns="http://www.w3.org/2000/svg" class="order-summary-toggle__icon">
                            <path d="M17.178 13.088H5.453c-.454 0-.91-.364-.91-.818L3.727 1.818H0V0h4.544c.455 0 .91.364.91.818l.09 1.272h13.45c.274 0 .547.09.73.364.18.182.27.454.18.727l-1.817 9.18c-.09.455-.455.728-.91.728zM6.27 11.27h10.09l1.454-7.362H5.634l.637 7.362zm.092 7.715c1.004 0 1.818-.813 1.818-1.817s-.814-1.818-1.818-1.818-1.818.814-1.818 1.818.814 1.817 1.818 1.817zm9.18 0c1.004 0 1.817-.813 1.817-1.817s-.814-1.818-1.818-1.818-1.818.814-1.818 1.818.814 1.817 1.818 1.817z"></path>
                        </svg>
                    </span>
                    <span class="order-summary-toggle__text order-summary-toggle__text--show">
                        <span>Show order summary</span>
                        <svg width="11" height="6" xmlns="http://www.w3.org/2000/svg" class="order-summary-toggle__dropdown" fill="#000"><path d="M.504 1.813l4.358 3.845.496.438.496-.438 4.642-4.096L9.504.438 4.862 4.534h.992L1.496.69.504 1.812z"></path></svg>
                    </span>
                    <span class="order-summary-toggle__text order-summary-toggle__text--hide">
                        <span>Hide order summary</span>
                        <svg width="11" height="7" xmlns="http://www.w3.org/2000/svg" class="order-summary-toggle__dropdown" fill="#000"><path d="M6.138.876L5.642.438l-.496.438L.504 4.972l.992 1.124L6.138 2l-.496.436 3.862 3.408.992-1.122L6.138.876z"></path></svg>
                    </span>
                    <span class="order-summary-toggle__total-recap total-recap" data-order-summary-section="toggle-total-recap">
                        <span class="total-recap__final-price skeleton-while-loading payment-due-price" >@FormatHelper.FormatCurrency(Model.Order.OrderTotal)</span>
                    </span>
                </span>
            </span>
        </button>
    </aside>

    <div class="content checkout" data-content="" aria-hidden="false">
        <div class="wrap">
            <div class="main">
                <header class="main__header" role="banner">

                    <nav aria-label="Breadcrumb">
                        <ul class="breadcrumb ">
                            <li class="breadcrumb__item breadcrumb__item--completed">
                                <a class="breadcrumb__link" href="/en/cart">Cart</a>
                                <svg class="icon-svg icon-svg--color-adaptive-light icon-svg--size-10 breadcrumb__chevron-icon" aria-hidden="true" focusable="false"> <use xlink:href="#chevron-right"></use> </svg>
                            </li>

                            <li class="breadcrumb__item step-1">
                                <a class="breadcrumb__link" href="/en/checkout/?step=1">
                                    Client Information
                                </a>
                                <svg class="icon-svg icon-svg--color-adaptive-light icon-svg--size-10 breadcrumb__chevron-icon" aria-hidden="true" focusable="false"> <use xlink:href="#chevron-right"></use> </svg>
                            </li>
                            <li class="breadcrumb__item step-2">
                                <a class="breadcrumb__link" href="/en/checkout/?step=2">Shipping</a>
                                <svg class="icon-svg icon-svg--color-adaptive-light icon-svg--size-10 breadcrumb__chevron-icon" aria-hidden="true" focusable="false"> <use xlink:href="#chevron-right"></use> </svg>
                            </li>
                            <li class="breadcrumb__item step-3">
                                <a class="breadcrumb__link" href="/en/checkout/?step=3">Payment</a>
                                <svg class="icon-svg icon-svg--color-adaptive-light icon-svg--size-10 breadcrumb__chevron-icon" aria-hidden="true" focusable="false"> <use xlink:href="#chevron-right"></use> </svg>
                            </li>
                            <li class="breadcrumb__item step-4">
                                <a class="breadcrumb__link" href="/en/checkout/?step=4">Order Confirmation</a>
                            </li>
                        </ul>
                    </nav>

                    <div class="shown-if-js" data-alternative-payments="">
                    </div>

                </header>
                <main class="main__content checkout__form" role="main">

                    <form id="checkout-form" class="pages">
                        <input type="hidden" name="__Subscription" value="">
                        <input type="hidden" name="__RequestVerificationToken" value="@Html.AntiForgeryTokenForAjaxPost()" />
                        <input type="hidden" name="__IsAuthenticated" value="@(isAuthenticated == true? "True":"False")" />
                        @{ var no = 1;}
                        @foreach (var item in Model.ShoppingCart.CartItems)
                        {
                            <input type="hidden" value="@item.ProductGroupId" name="Order.OrderItems[@(no-1)].ProductGroupId">
                            <input type="hidden" value="@item.Product.Id" name="Order.OrderItems[@(no-1)].ProductId">
                            <input type="hidden" value="@item.Count" name="Order.OrderItems[@(no-1)].Quantity">
                            no++;
                        }

                        <input type="hidden" value="@Model.Order.OrderTransportFee" name="Order.OrderTransportFee">
                        <input type="hidden" value="@Model.Order.OrderDiscount" name="Order.OrderDiscount">
                        <input type="hidden" value="@Model.Order.OrderTotal" name="Order.OrderTotal">

                        <div class="page page--1" data-step="step-1">
                            <header class="header">
                                <h2>Customer information</h2>
                                @if (isAuthenticated)
                                {
                                    <span>Hi <b>@UserManager.User.FriendlyName</b>, You are signed as name <b>@UserManager.User.Username</b>. <br /> Try switch account if you  want check out other information, <a href="@Url.Action("Logoff", "Account", new { returnUrl = "/" + DataCurrentHelper.UniqueSeoCode + "/checkout/?step=2" })"> Sign out</a> here</span>
                                }
                                else
                                {
                                    <span class="extra">
                                        Already have an account?
                                        @{ Html.RenderPartial("~/Views/Default/Shared/Component/_LoginCheckoutPartial.cshtml"); }
                                    </span>

                                }
                            </header>
                            <p class="microcopy">Please tell us to which email address we can send the  order confirmation.</p>

                            <div class="f">

                                <div class="input">

                                    @if (isAuthenticated)
                                    {
                                        <label for="AsMember_UserName" class="input__label">Email || IndetifyName <span class="required">(required)</span></label>
                                        @Html.TextBoxFor(model => model.AsMember.UserName, new { placeholder = "", @class = "input__input", type = "text", @readonly = true, @style = "background: #fafafa;" })

                                    }
                                    else
                                    {
                                        <label for="AsGuest_UserName" class="input__label">Email <span class="required">(required)</span></label>
                                        @Html.TextBoxFor(model => model.AsGuest.UserName, new { placeholder = "", @class = "input__input", type = "email" })
                                        @Html.ValidationMessageFor(model => model.AsGuest.UserName)
                                    }
                                    <div class="input__error">Please enter a valid email address.</div>
                                </div>

                                <label for="newsletter" class="control">
                                    <input type="checkbox" id="newsletter" class="control__input" />
                                    <span class="control__label">Please keep me informed about <b>offers</b> and <b>discounts</b>.</span>
                                </label>

                            </div>

                            <header class="header">
                                <h2>Shipping address</h2>
                            </header>
                            <p class="microcopy">Please tell us where to ship your order.</p>
                            <div class="f">
                                <div class="f60">
                                    <div class="input input--select">
                                        <label for="Order.Customer.MainAddress.Gender" class="input__label">Title <span class="required">(required)</span></label>
                                        <select class="input__input" name="Order.Customer.MainAddress.Gender" required>
                                            <optgroup label="Title">
                                                <option @(Model.Order.Customer.MainAddress.Gender == Gender.Male ? "selected" : "") value="1">Mr</option>
                                                <option @(Model.Order.Customer.MainAddress.Gender == Gender.Female ? "selected" : "") value="2">Miss</option>
                                            </optgroup>
                                            <optgroup label="Other">
                                                <option @(Model.Order.Customer.MainAddress.Gender == 0 ? "selected" : "") value="0">Secret</option>
                                            </optgroup>
                                        </select> <!-- /.form-control -->
                                    </div>
                                </div>

                            </div>
                            <div class="f">
                                <div class="f60">
                                    <div class="input">
                                        <label for="Order_Customer_MainAddress_FirstName" class="input__label">First name <span class="required">(required)</span></label>
                                        @Html.TextBoxFor(model => model.Order.Customer.MainAddress.FirstName, new { required = "required", placeholder = "", @class = "input__input" })
                                        @Html.ValidationMessageFor(model => model.Order.Customer.MainAddress.FirstName)
                                        <div class="input__error">Please enter a valid address.</div>
                                    </div>
                                </div>
                                <div class="f40">
                                    <div class="input">
                                        <label for="Order_Customer_MainAddress_LastName" class="input__label">Last name. <span class="optional">(optional)</span></label>
                                        @Html.TextBoxFor(model => model.Order.Customer.MainAddress.LastName, new { placeholder = "", @class = "input__input" })
                                        @Html.ValidationMessageFor(model => model.Order.Customer.MainAddress.LastName)
                                    </div>
                                </div>

                                <div class="input">
                                    <label for="Order_Customer_MainAddress_PhoneNumber" class="input__label">Phone number <span class="required">(required)</span></label>
                                    @Html.TextBoxFor(model => model.Order.Customer.MainAddress.PhoneNumber, new { placeholder = "", @class = "input__input phone", required = "required" })
                                    @Html.ValidationMessageFor(model => model.Order.Customer.MainAddress.PhoneNumber)
                                </div>

                                <div class="input">
                                    <label for="Order_Customer_MainAddress_Email" class="input__label">E-mail <span class="optional">(optional)</span></label>
                                    @Html.TextBoxFor(model => model.Order.Customer.MainAddress.Email, new { placeholder = "", @class = "input__input", required = "required", type = "email" })
                                    @Html.ValidationMessageFor(model => model.Order.Customer.MainAddress.Email)
                                </div>

                                <!--  <div class="f40"> -->
                                <div class="input">
                                    <label for="Order_Customer_MainAddress_Address1" class="input__label">Address line 1 <span class="required">(required)</span></label>
                                    @Html.TextBoxFor(model => model.Order.Customer.MainAddress.Address1, new { placeholder = "", @class = "input__input", required = "required" })
                                </div>
                                <!--             </div> -->
                                <div class="collapser">
                                    <div class="collapser__label"><a href="#">Add another address line</a></div>
                                    <div class="collapser__content">
                                        <div class="input">
                                            <label for="Order_Customer_MainAddress_Address2" class="input__label">Address line 2 <span class="optional">(optional)</span></label>
                                            @Html.TextBoxFor(model => model.Order.Customer.MainAddress.Address2, new { placeholder = "", @class = "input__input" })
                                            <div class="input__error">Please enter a valid address.</div>
                                        </div>
                                    </div>
                                </div>
                                <div class="f region">
                                    <div class="f50">
                                        <div class="input input--select">
                                            <select class="step1" style="display:none">
                                                <!-- other options -->
                                            </select>
                                            <label for="Order_Customer_MainAddress_StateProvinceId" class="input__label">State/Province <span class="required">(required)</span></label>
                                            <select name="Order.Customer.MainAddress.StateProvinceId" data-id="@Model.Order.Customer.MainAddress.StateProvinceId" id="Order_Customer_MainAddress_StateProvinceId" class="input__input step2">
                                                <option value="">Select Province</option>
                                            </select>
                                        </div>
                                    </div>
                                    <div class="f50">
                                        <div class="input input--select">
                                            <label for="Order_Customer_MainAddress_DistrictId" class="input__label">District <span class="required">(required)</span></label>
                                            <select name="Order.Customer.MainAddress.DistrictId" data-id="@Model.Order.Customer.MainAddress.DistrictId" id="Order_Customer_MainAddress_DistrictId" class="input__input step3">
                                                <option disabled value="">Select District</option>
                                            </select>
                                        </div>
                                    </div>
                                </div>


                            </div>
                            <div class="f">
                                <label for="billing-same" class="control">
                                    <input type="checkbox" id="billing-same" class="control__input" checked />
                                    <span class="control__label">My <b>billing information</b> is the same as my <b>shipping information</b>.</span>
                                </label>
                            </div>

                            <div id="billing">

                                <header class="header">
                                    <h2>Billing address</h2>
                                </header>
                                <p class="microcopy">The person that will be billed for this order, if different from the shipping receiver.</p>

                                <div class="f">
                                    <div class="f60">
                                        <div class="input input--select">
                                            <label for="Order.Customer.BillingAddress.Gender" class="input__label">Title <span class="required">(required)</span></label>
                                            <select class="input__input" name="Order.Customer.BillingAddress.Gender" required>
                                                <optgroup label="Title">
                                                    <option @(Model.Order.Customer.BillingAddress.Gender == Gender.Male ? "selected" : "") value="1">Mr</option>
                                                    <option @(Model.Order.Customer.BillingAddress.Gender == Gender.Female ? "selected" : "") value="2">Miss</option>
                                                </optgroup>
                                                <optgroup label="Other">
                                                    <option @(Model.Order.Customer.BillingAddress.Gender == 0 ? "selected" : "") value="0">Secret</option>
                                                </optgroup>
                                            </select> <!-- /.form-control -->
                                        </div>
                                    </div>

                                </div>
                                <div class="f">
                                    <div class="f60">
                                        <div class="input">
                                            <label for="Order_Customer_BillingAddress_FirstName" class="input__label">First name <span class="required">(required)</span></label>
                                            @Html.TextBoxFor(model => model.Order.Customer.BillingAddress.FirstName, new { placeholder = "", @class = "input__input" })
                                            @Html.ValidationMessageFor(model => model.Order.Customer.BillingAddress.FirstName)
                                            <div class="input__error">Please enter a valid address.</div>
                                        </div>
                                    </div>
                                    <div class="f40">
                                        <div class="input">
                                            <label for="Order_Customer_BillingAddress_LastName" class="input__label">Last name. <span class="optional">(optional)</span></label>
                                            @Html.TextBoxFor(model => model.Order.Customer.BillingAddress.LastName, new { placeholder = "", @class = "input__input" })
                                            @Html.ValidationMessageFor(model => model.Order.Customer.BillingAddress.LastName)
                                        </div>
                                    </div>

                                    <div class="input">
                                        <label for="Order_Customer_BillingAddress_PhoneNumber" class="input__label">Phone number <span class="required">(required)</span></label>
                                        @Html.TextBoxFor(model => model.Order.Customer.BillingAddress.PhoneNumber, new { placeholder = "", @class = "input__input phone", })
                                        @Html.ValidationMessageFor(model => model.Order.Customer.BillingAddress.PhoneNumber)
                                    </div>

                                    <div class="input">
                                        <label for="Order_Customer_BillingAddress_Email" class="input__label">E-mail <span class="optional">(optional)</span></label>
                                        @Html.TextBoxFor(model => model.Order.Customer.BillingAddress.Email, new { placeholder = "", @class = "input__input", type = "email" })
                                        @Html.ValidationMessageFor(model => model.Order.Customer.BillingAddress.Email)
                                    </div>

                                    <!--  <div class="f40"> -->
                                    <div class="input">
                                        <label for="Order_Customer_BillingAddress_Address1" class="input__label">Address line 1 <span class="required">(required)</span></label>
                                        @Html.TextBoxFor(model => model.Order.Customer.BillingAddress.Address1, new { placeholder = "", @class = "input__input" })
                                    </div>
                                    <!--             </div> -->
                                    <div class="collapser">
                                        <div class="collapser__label"><a href="#">Add another address line</a></div>
                                        <div class="collapser__content">
                                            <div class="input">
                                                <label for="Order_Customer_BillingAddress_Address2" class="input__label">Address line 2 <span class="optional">(optional)</span></label>
                                                @Html.TextBoxFor(model => model.Order.Customer.BillingAddress.Address2, new { placeholder = "", @class = "input__input" })
                                                <div class="input__error">Please enter a valid address.</div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="f region">
                                        <div class="f50">
                                            <div class="input input--select">
                                                <select class="step1" style="display:none">
                                                    <!-- other options -->
                                                </select>
                                                <label for="Order_Customer_BillingAddress_StateProvinceId" class="input__label">State/Province <span class="required">(required)</span></label>
                                                <select name="Order.Customer.BillingAddress.StateProvinceId" data-id="@Model.Order.Customer.BillingAddress.StateProvinceId" id="Order_Customer_BillingAddress_StateProvinceId" class="input__input step2">
                                                    <option value="">Select Province</option>
                                                </select>
                                            </div>
                                        </div>
                                        <div class="f50">
                                            <div class="input input--select">
                                                <label for="Order_Customer_BillingAddress_DistrictId" class="input__label">District <span class="required">(required)</span></label>
                                                <select name="Order.Customer.BillingAddress.DistrictId" data-id="@Model.Order.Customer.BillingAddress.DistrictId" id="Order_Customer_BillingAddress_DistrictId" class="input__input step3">
                                                    <option disabled value="">Select District</option>
                                                </select>
                                            </div>
                                        </div>
                                    </div>


                                </div>

                            </div>

                            <div class="form__footer">
                                <button class="btn btn--primary" data-next>
                                    <span class="btn__label">Continue to Shipping Method</span>
                                    <svg class="btn__loader" viewBox="25 25 50 50"><circle cx="50" cy="50" r="20" fill="none" class="path"></circle></svg>
                                </button>
                                <a class="btn btn--transparent secondary" href="/en/cart">
                                    <svg class="btn__icon"><use xlink:href="#icon-navigate_before"></use></svg>
                                    <span class="btn__label">Return to Cart</span>
                                    <svg class="btn__loader" viewBox="25 25 50 50"><circle cx="50" cy="50" r="20" fill="none" class="path"></circle></svg>
                                </a>
                            </div>

                        </div>
                        <div class="page page--2" data-step="step-2">


                            <div class="content-box">
                                <div role="table" class="content-box__row content-box__row--tight-spacing-vertical">
                                    <div role="row" class="review-block">
                                        <div class="review-block__inner">
                                            <div role="rowheader" class="review-block__label">
                                                Information
                                            </div>
                                            <div role="cell" class="review-block__content">
                                                <b>P: </b><bdo dir="ltr" class="phone"></bdo>
                                                <b>E: </b><bdo dir="ltr" class="email"></bdo>
                                                <div>
                                                    <b style="float: left;margin-right: 3px;">A: </b><address class="address address--tight">

                                                    </address>
                                                </div>
                                            </div>
                                        </div>
                                        <div role="cell" class="review-block__link">
                                            <button type="button" class="link--small" data-href="step-1">
                                                <span aria-hidden="true">Change</span>
                                                <span class="visually-hidden">Change contact information</span>
                                            </button>
                                        </div>
                                    </div>

                                </div>
                            </div>
                            <header class="header">
                                <h2>Shipping Method</h2>
                            </header>
                            <p class="microcopy">Select the shipping method you would like to use.</p>

                            <div class="f f--no-margin">
                                @{
                                    var i = 0;
                                }
                                @foreach (var item in Model.ShippingMethods.OrderBy(e => e.Fees))
                                {

                                    <label for="shipping-@item.Id" class="control block @((i == 0) ? "is-selected":"")">
                                        <input type="radio" id="shipping-@item.Id" required data-val="@item.Fees" data-name="@item.Name" data-desc="@Html.Raw(item.Description)" name="Order.ShippingMethodId" @(i == 0 ? "checked" : "") value="@item.Id">
                                        <span class="control__label">
                                            @item.Name
                                            <span class="microcopy">@Html.Raw(item.Description)</span>
                                        </span>
                                        <span class="control__extra primary">@FormatHelper.FormatCurrency(item.Fees)</span>
                                    </label>
                                    <!-- /.shipping-item -->
                                    i++;
                                }

                            </div>

                            <div class="form__footer">
                                <button class="btn btn--primary " data-next>
                                    <span class="btn__label">Continue to Payment Method</span>
                                    <svg class="btn__loader" viewBox="25 25 50 50"><circle cx="50" cy="50" r="20" fill="none" class="path"></circle></svg>
                                </button>
                                <button class="btn btn--transparent secondary " data-prev>
                                    <svg class="btn__icon"><use xlink:href="#icon-navigate_before"></use></svg>
                                    <span class="btn__label">Return to your information</span>
                                    <svg class="btn__loader" viewBox="25 25 50 50"><circle cx="50" cy="50" r="20" fill="none" class="path"></circle></svg>
                                </button>
                            </div>

                        </div>
                        <div class="page page--3" data-step="step-3">
                            <div class="content-box">
                                <div role="table" class="content-box__row content-box__row--tight-spacing-vertical">
                                    <div role="row" class="review-block">
                                        <div class="review-block__inner">
                                            <div role="rowheader" class="review-block__label">
                                                Information
                                            </div>
                                            <div role="cell" class="review-block__content">
                                                <b>P: </b><bdo dir="ltr" class="phone"></bdo>
                                                <b>E: </b><bdo dir="ltr" class="email"></bdo>
                                                <div>
                                                    <b style="float: left;margin-right: 3px;">A: </b><address class="address address--tight">

                                                    </address>
                                                </div>
                                            </div>
                                        </div>
                                        <div role="cell" class="review-block__link">
